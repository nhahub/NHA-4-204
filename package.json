{
  "name": "careergps",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "dotenv": "^17.2.2",
      "zod": "^4.1.13",
      "@trpc/server": "^11.7.2",
      "better-auth": "^1.4.18",
      "@types/express": "^5.0.1",
      "@trpc/client": "^11.7.2"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "db:push": "turbo -F @careergps/db db:push",
    "db:studio": "turbo -F @careergps/db db:studio",
    "db:generate": "turbo -F @careergps/db db:generate",
    "db:migrate": "turbo -F @careergps/db db:migrate"
  },
  "dependencies": {
    "@careergps/env": "workspace:*",
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@careergps/config": "workspace:*",
    "@types/node": "^22.13.14",
    "turbo": "^2.6.3",
    "typescript": "^5"
  },
  "packageManager": "bun@1.3.6"
}
